@{
    ViewBag.Title = "Taxonomy";
	ViewBag.SubTitle = "Edit";
}

@section Styles {
    @Html.Raw(Styles.Render("~/bundles/select2css"))
}

@section Scripts {
    @Html.Raw(Scripts.Render("~/bundles/select2"))
	@Html.Raw(Scripts.Render("~/bundles/knockout"))
	@Html.Raw(Scripts.Render("~/bundles/taxonomy/edit"))

	<script type="text/javascript">
		$(function() {
			wmis.taxonomy.edit.initialize({
				taxonomyKey: @ViewBag.Key
			});
		});
	</script>
}

@{ Html.RenderPartial("EditForm"); }