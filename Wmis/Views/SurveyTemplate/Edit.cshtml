@{
	ViewBag.Title = "Survey Template";
	ViewBag.SubTitle = "Edit";
}

@section Styles {
    @Html.Raw(Styles.Render("~/bundles/content/datatables"))
    @Html.Raw(Styles.Render("~/bundles/select2css"))
}

@section Scripts {
    @Html.Raw(Scripts.Render("~/bundles/select2"))
	@Html.Raw(Scripts.Render("~/bundles/knockout"))
	@Html.Raw(Scripts.Render("~/bundles/surveytemplate/edit"))

	<script type="text/javascript">
		$(function() {
		    wmis.surveytemplate.edit.initialize({
		        surveyTemplateKey: @ViewBag.Key
			});
		});
	</script>
}

@{ Html.RenderPartial("EditForm"); }